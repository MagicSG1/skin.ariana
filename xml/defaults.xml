    <sliderheight>24</sliderheight>
    <texturesliderbar border="3,0,3,0">HUD/SliderBG.png</texturesliderbar>
    <textureslidernib>HUD/SliderNib.png</textureslidernib>
    <textureslidernibfocus colordiffuse="Selected">HUD/SliderNib.png</textureslidernibfocus>
