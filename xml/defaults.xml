    <selectedcolor>Selected</selectedcolor>
    <disabledcolor>DisabledDark</disabledcolor>
    <textoffsetx>20</textoffsetx>
    <align>left</align>
    <aligny>center</aligny>
    <reverse>yes</reverse>
    <pulseonselect>false</pulseonselect>
    <include>FocusButtonsAnimation</include>
